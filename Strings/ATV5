/*Escreva um programa que verifique se duas strings são iguais.*/

#include <stdio.h>
#include <string.h>

	int main(){

	char str[30] = "Universidade Positivo";
	
	if(strcmp(str, "Universidade Positivo") == 0){
	printf("As strings são iguais.");
	}
	else{printf("As strings são diferentes.");
	}
	
	return 0;
	}
