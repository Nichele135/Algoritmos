/*Escreva um programa que leia uma frase de até 30 caracteres. Em seguida, escreva a frase sem osespaços em branco.*/

#include <stdio.h>
#include <string.h>

	int main(){

	char i, str[31];
	
	printf("Informe a frase: ");
	scanf(" %20[^\n]", str);
	
	for(i = 0; str[i] != '\0'; i++){
		if(str[i] != ' '){
			printf("%c", str[i]);
		}
	}
	
	return 0;
	}
